{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "mock/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "index.html"
        }
    ],
    "installCommand": "pnpm install --production=false",
    "buildCommand": "npx vite build"
}